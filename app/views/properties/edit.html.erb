<center>
  <h2>Edit <%= @property.title %></h2>

  <%= form_with model: @property do |f| %>
    <%= f.label :title, "Name: " %>
    <%= f.text_field :title %>
    <br><br>
    <%= f.submit "Update Property", class: "button" %>
  <% end %>
  <br>
  <%= link_to "Delete this property", delete_property_path(property: @property), class: "button small discrete" %>

</center>